    "eslint": "^5.16.0",
    "file-loader": "^3.0.1",
    "hyperapp": "^1.2.9",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.0.3",
    "simplejsonconf": "^2.0.5",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.6.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  }
